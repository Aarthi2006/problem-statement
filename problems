1)public class Main {
    public static void main(String[] args) {
        int age = 25; 
        int ageResult;

        if (age < 0 || age > 101) {
            ageResult = -1;
        } else if (age <= 5) {
            ageResult = 0;
        } else if (age <= 12) {
            ageResult = 1;
        } else if (age <= 19) {
            ageResult = 2;
        } else if (age <= 50) {
            ageResult = 3;
        } else if (age <= 60) {
            ageResult = 4;
        } else {
            ageResult = 5;
        }

        System.out.println("Age: " + age);
        System.out.println("Age Result: " + ageResult);
    }
}


2)
public class Main {
    public static void main(String[] args) {
        int sqdnumber = 234;
        int sqdNumber_result = 0;

        while (sqdnumber > 0) {
            int digit = sqdnumber % 10;
            sqdNumber_result += digit * digit;
            sqdnumber /= 10;
        }

3)
public class Main {
    public static void main(String[] args) {
        int number = 3; 
        int cycle_no = 0;
        int finalNumber = number;

        while (finalNumber != 1 && cycle_no < 10) {
            int sum = 0;
            int temp = finalNumber;

        
            while (temp > 0) {
                int digit = temp % 10;
                sum += digit * digit;
                temp /= 10;
            }

            
            finalNumber = sum;
            cycle_no++;
        }
        if (finalNumber == 1) {
            System.out.println("The number is a Happy Number.");
        } else {
            System.out.println("The number is NOT a Happy Number after 10 cycles.");
        }

        System.out.println("Final number after cycles: " + finalNumber);
        System.out.println("Number of cycles performed: " + cycle_no);
    }
}

        System.out.println("Sum of squares of digits: " + sqdNumber_result);
    }
}



